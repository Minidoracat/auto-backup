{
    "source_directory": "/app/source",
    "target_directory": "/app/backup",
    "backup_count": 7,
    "compress": true,
    "compress_format": "tar.gz",
    "schedule_mode": "cron",
    "schedule": {
        "cron": {
            "schedule_time": "01:00"
        },
        "interval": {
            "days": 0,
            "hours": 0,
            "minutes": 60,
            "seconds": 0
        }
    }
}